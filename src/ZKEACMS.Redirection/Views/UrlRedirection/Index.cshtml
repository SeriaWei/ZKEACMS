@using ZKEACMS.Redirection.Models
@model UrlRedirect
@{
    Style.Reqiured("dataTable").AtHead();
    Script.Reqiured("dataTable").AtFoot();

    Script.Reqiured("datepicker").AtFoot();
    Style.Reqiured("datepicker").AtHead();

}
@if (Authorizer.Authorize(ZKEACMS.Redirection.PermissionKeys.ManageUrlRedirect))
{
    <div id="toolBar">
        <a href="@Url.Action("Create")" class="btn btn-primary btn-sm">@L("新建")</a>
    </div>
}
<grid delete-able="Authorizer.Authorize(ZKEACMS.Redirection.PermissionKeys.ManageUrlRedirect)" edit-able="Authorizer.Authorize(ZKEACMS.Redirection.PermissionKeys.ManageUrlRedirect)"></grid>
